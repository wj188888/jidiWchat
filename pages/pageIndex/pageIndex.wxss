.questionLi .changefault {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 30rpx;
  height: 30rpx;
}

.indexFooter {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  text-align: center;
  box-sizing: border-box;
}

.indexFooter .left {
  width: 72%;
  float: left;
  font-size: 28rpx;
}

.indexFooter .left .total {
  color: #ff3e34;
  font-size: 32rpx;
}

.indexFooter .left .remark {
  margin-left: 20rpx;
  color: #999;
  font-size: 22rpx;
}

.indexFooter .right {
  width: 28%;
  background: #ff3e34;
  color: #fff;
  font-size: 32rpx;
  float: right;
}

.detailLi {
  position: relative;
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  box-sizing: border-box;
  align-items: center;
  border: 1px solid #e4e4e4;
  border-radius: 8rpx;
  height: 80rpx;
  font-size: 28rpx;
  margin: 20rpx auto;
  padding: 0 30rpx;
}

.detailLi .gou {
  position: absolute;
