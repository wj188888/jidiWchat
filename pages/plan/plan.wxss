  /* pages/plan/plan.wxss */
  
  .mobileinfo {
  	background: #fff;
  	margin-bottom: 20rpx;
  	float: left;
  	width: 100%;
  	box-sizing: border-box
  }
  
  .mobileinfo .top {
  	padding: 20rpx 20rpx;
  	width: 100%;
  	float: left;
  	box-sizing: border-box
  }
  
  .mobileinfo .top .left {
  	width: 110rpx;
  	height: 110rpx;
  	border: 1px solid #ccc;
  	box-sizing: border-box;
  	text-align: center;
  	float: left;
  	margin-right: 20rpx
  }
  
  .mobileinfo .top .left .img {
  	width: 50rpx;
  	height: 95rpx;
  	margin-top: 5rpx;
    vertical-align: middle
  }
  
  .mobileinfo .top .right {
  	float: left;
  	width: 78%
  }
  
  .mobileinfo .top .right .gzclass {
  	margin: 5rpx 0;
    display: block
  }
  
  .mobileinfo .top .right .gzclass text {
  	font-size: 32rpx
  }
  
  .mobileinfo .top .right .gzmodel {
  	color: #999
  }
  
  .mobileinfo .top .right .price {
  	float: right
  }
  
  .mobileinfo .bottom {
  	width: 100%;
  	height: 70rpx;
  	line-height: 70rpx;
  	border-top: 1px solid #ccc;
  	float: left;
  	padding: 0 40rpx;
  	text-align: right;
  	box-sizing: border-box
  }
  
  .mobileinfo .bottom .total {
  	font-size: 34rpx;
  	color: #ff3e34
  }
  
  .commontitle {
  	float: left;
  	margin-bottom: 30rpx;
  	box-sizing: border-box
  }
  
  .planbox {
  	width: 100%;
  	background: #fff;
  	padding: 20rpx;
  	box-sizing: border-box;
  	float: left
  }
  
  .planbox2 {
  	width: 100%;
  	background: #fff;
  	padding: 20rpx 20rpx 0 20rpx;
  	box-sizing: border-box;
  	float: left;

  }
  
  .litbox {
  	height: 70rpx;
  	line-height: 70rpx;
  	text-align: center;
  	border: 1px solid #ff3e34;
  	border-radius: 5px;
  	padding: 0 30rpx;
  	display: inline-block;
  	margin-right: 20rpx;
  	margin-left: 3.6%;
 
  }
  
  .choosebox {
  	width: 100%;
  	float: left
  }
  
  .litbox2 {
  	height: 70rpx;
  	line-height: 70rpx;
  	text-align: center;
  	border-radius: 5px;
  	display: inline-block;
  	width: 26%;
  	border: 1px solid #ccc;
  	margin: 0 3.5%;
  	box-sizing: border-box;
  	margin-bottom: 20rpx;
  }
  
  .tab-nav {
  	width: 25%;
  	text-align: center;
  	float: left;
   padding-bottom: 10px

  }
  .tab-nav .iconfont{ font-size: 30px; margin-top: 0; line-height: 30px}
  
  .tab-nav text {
  	display: block;
   font-size: 13px;
   color: #999
  }
  
  .tab-nav .img {
  	height: 60rpx;
  	line-height: 60rpx;
  	width: 100%
  }
  
  .tab-nav .img image {
  	vertical-align: middle
  }
  
  .wxbtn {
  	width: 90%;
  	height: 98rpx;
  	border-radius: 5px;
  	background: #ff3e34;
  	color: #fff;
  	font-size: 36rpx;
  	margin: 10rpx 0 20rpx 0;
  	float: left;
  	margin-left: 5%
  }
  
  .ques-pop {
  	width: 80%;
  	background: #fff;
  	position: fixed;
  	left: 10%;
  	top: 10%;
  	height: 800rpx;
  	border-radius: 20rpx;
  	z-index: 9999;
  }
  
  .gz-box {
  	padding: 5% 34rpx 0 5%;
  	box-sizing: border-box
  }
  
  .gz-box .active {
  	border-color: red
  }

  .gz-box view {
  	width: 100%;
  	height: 76rpx;
  	line-height: 76rpx;
  	border: 1px solid #ccc;
  	border-radius: 5px;
  	padding: 0 20rpx;
  	box-sizing: border-box;
  	margin-top: 20rpx
  }
  
  .gz-box view .fr {
  	float: right
  }
  
  .ques-pop .btn-group {
  	margin-top: 30rpx
  }
  
  .tab-box {
  	width: 100%;
  	float: left;
  }
  
  .tab-box .tab-nav {
  	position: relative
  }
  
  .tab-box .on text {
  	color: #ff3e34
  }
  .tab-box .on .iconfont{color: #ff3e34}
  .tab-box .on .arr {
  	height: 0;
  	width: 0;
  	overflow: hidden;
  	font-size: 0;
  	line-height: 0;
  	border-color: transparent transparent #f4b1b2 transparent;
  	border-style: solid;
  	border-width: 14rpx;
  	position: absolute;
  	left: 40%;
  	bottom: 0
  }
  
  .tab-content {
  	width: 100%;
  	float: left
  }
  
  .tab-content .listbox {
  	width: 100%;
  	border-top: 1px solid #ccc;
  	padding: 0rpx 20rpx;
  	box-sizing: border-box;
  	background: #fff;

  	display: inline-block;
  float: left
  }
  
  .tab-content .listbox .words {
  	width: 80%;
    line-height: 38px;
    float: left;
	font-size: 30rpx;
  color: #999;
  }
  
  .tab-content .listbox .words2 {
  	color: #999;
  line-height: 19px;
  width: 80%;
  float: left;
   	font-size: 30rpx;

  }
  
  .tab-content .notice {
  	width: 100%;
  	padding: 5rpx 30rpx;
  	box-sizing: border-box;
  	background: #f4b1b2;
  	color: #d72620;
  	text-align: center;
  	font-size: 24rpx
  }
  
  .tab-content .listbox .fl {
  	font-size: 24px;
  	color: #ff3e34;
  	margin-right: 30rpx;
  	float: left
  }
  
  .tab-content .listbox .fr {
  	float: right;
    margin-top: 10rpx
  }


  
  .tab-content .listbox .text-input {
   width:70%;float:left; height:38px;line-height: 38px;
  	border: none;
  	font-size: 30rpx
  }
  
  .tab-content .listbox:first-child {
  	border-top: none
  }
  
  .tab-content .center {
  	background: #fff;
  	margin: 20rpx 0;
  	width: 100%;
  	padding: 20rpx;
  	box-sizing: border-box;
  	float: left
  }
  
  .tab-content .center .title {
  	font-size: 30rpx;
  	display: block
  }
  
  .tab-content .center .bzwords {
  	width: 100%;
  	border: 1px solid #ccc;
  	border-radius: 5px;
    min-height:200rpx;
  	margin: 20rpx 0;
  	padding: 20rpx;
  	box-sizing: border-box;
    z-index: 1
  }
  
  .tab-content .center .descr {
  	line-height: 50rpx;
    font-size: 26rpx
  }
  
  .words {
  	height: 100%
  }
  
  .ques-pop {
  	height: auto;
  	padding-bottom: 30rpx
  }
 .tab-content .listbox  .text_color{ color: #4d4d4d!important} 
  .tab-nav1.on{background: url(https://xcx.yunbaite.com/static/img/way_1.png)}
  .tab-content .remark{ margin: 10rpx 0; border-top: none; height: 40px;line-height: 40px; float: left}
  .tab-content .remark input{  font-size:30rpx; height: 40px; }
  .tab-content .remark label{ display: inline-block;float: left; margin-right: 20rpx; color: #ff3e34;font-size:30rpx;}