::-webkit-scrollbar{ width: 0; height: 0; color: transparent; }

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  overflow-x: hidden
} 
Page {
  background: #eeedf2;

  font-size: 28rpx;

  overflow-x: hidden;
  overflow-y: auto;
  color: #4d4d4d
}
.commontitle{ height: 40rpx; width:100%; display: block; line-height: 40rpx; border-left: 12rpx solid #ff3e34; padding-left: 20rpx; margin-bottom:0rpx; font-size: 30rpx;box-sizing: border-box}
.btn-group .btn{ width: 33%; height: 70rpx;line-height: 70rpx; font-size: 32rpx;display: inline-block;background: #cccccc;color: #fff;}
.btn-group{ text-align: center; margin-top: 60rpx;float: left; width: 100%; padding: 0 50rpx; box-sizing: border-box}
.btn-group .sure{ background:#ff3126; }
.popmask{ width: 100%; height: 100%; position:fixed; left: 0;top: 0;background: rgba(0, 0, 0, 0.5); z-index: 9998}
.change-pop,.ques-pop,timepop{ width: 80%; background: #fff; position:fixed; left: 10%; top: 10%; height: 800rpx; border-radius: 20rpx; z-index: 9999;}
.pickerbox{ background: #fff; border-radius: 20rpx; text-align: center; font-size: 34rpx; line-height: 50px; margin-top: 10px;width: 100%; height:600rpx;}

/* 常用 */
.clearfix:before,.clearfix:after { content: " "; display: block; height: 0; clear: both; visibility: hidden; }
.ellipsis{overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.ellipsis-2{overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-box-orient: vertical;  -webkit-line-clamp: 2;}
.clearlist{ margin: 0; padding: 0; list-style: none;}
.fl{ float: left}
.fr{ float: right}

/* 页面容器 */
.weui-content{}
.weui-content-padded{ margin:15px;}

/* 上下导航 */
.weui-bar{ width: 100%; height: 45px; line-height: 45px; background-color: #FFF; box-sizing: border-box; position: fixed; left: 0; z-index: 9;}
.weui-bar:after{ content: ""; height: 1px; background-color: #CCC; -webkit-transform: scaleY(0.5); position: absolute; left: 0; right: 0;}

.weui-bar-header{  top: 0; }
.weui-bar-header:after{ bottom: 0;}
.weui-bar-header~.weui-content{padding-top: 45px;}

.weui-bar-footer{ height: 45px; bottom: 0;}
.weui-bar-footer:after{ top: 0;}
.weui-bar-footer~.weui-content{ padding-bottom: 45px;}

/* 轮播小圆点 */
swiper .wx-swiper-dot{ width:8px; height: 8px;   background-color: rgba(0,0,0,0.5) !important;}
swiper .wx-swiper-dot-active{ background-color: #FFF !important;}





