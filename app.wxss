Page {
  width: 100%;
  height: 100%;
  background: #eeedf2;
  font-size: 28rpx;
  overflow-x: hidden;
  overflow-y: auto;
  color: #4d4d4d;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0;
  box-sizing: border-box;
  overflow-x: hidden;
}

/*弹窗蒙层  */

.popmask {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9998;
}

/*高度固定弹窗  */

.dialogBox {
  width: 80%;
  background: #fff;
  position: fixed;
  border-radius: 20rpx;
  z-index: 9999;
  top: 13%;
  left: 10%;
  height: 70%;
}

/*弹窗内容 固定高度  */

.pickerbox {
  background: #fff;
  border-radius: 20rpx;
  text-align: center;
  font-size: 34rpx;
  line-height: 50px;
  margin-top: 10px;
  width: 100%;
  height: 80%;
}

/*弹窗按钮 固定高度  */

.popbtn {
  text-align: center;
  width: 100%;
  height: 20%;
  padding: 0 50rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
}

.popbtn .btn {
  width: 33.33%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 32rpx;
  display: inline-block;
  background: #ccc;
  color: #fff;
}

.popbtn .okbtn {
  background: #ff3126;
}

/*高度不固定弹窗  */

.adaptdialogbox {
  position: fixed;
  left: 50%;
  top: 45%;
  width: 80%;
  border-radius: 8rpx;
  -webkit-transform: translateX(-50%) translateY(-45%);
  transform: translateX(-50%) translateY(-45%);
  background-color: #fff;
  z-index: 9999;
}

/*弹窗内容 不固定高度  */

.dadaptpopcontent {
  min-height: 200rpx;
  max-height: 600rpx;
  overflow: auto;
  padding: 30rpx 40rpx 0;
  box-sizing: border-box;
}

.adaptpopbtn {
  text-align: center;
  width: 100%;
  height: 120rpx;
  padding: 0 50rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
}

.adaptpopbtn .btn {
  width: 33.33%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 32rpx;
  display: inline-block;
  background: #ccc;
  color: #fff;
}

.adaptpopbtn .okbtn {
  background: #ff3126;
}

.f-c-999 {
  color: #999;
}
